    "brittle": "^3.4.0",
    "compact-encoding": "^2.13.0",
    "jsdoc-to-markdown": "^8.0.1",
    "localdrive": "^1.11.4",
    "the-answer": "^1.0.0",
    "rxjs": "^7.8.1"
  },
  "dependencies": {
    "awilix": "^10.0.1",
    "simplified-awilix": "^0.2.8"
